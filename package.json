{
  "name": "airfi-home",
  "version": "1.0.0",
  "description": "Airfi Home",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel start-proxy start-server",
    "start-proxy": "webpack-dev-server --env.PORT=3010 --env.TO=3011 --open --inline",
    "start-server": "cross-env PORT=3011 node --inspect=9229 src/index.js",
    "start-prod": "npm run build-prod && node src/index.js",
    "flow": "flow check",
    "build-prod": "cross-env NODE_ENV=production PORT=3011 webpack -p",
    "test": "jest --watch"
  },
  "author": "Karim Abdelcadir",
  "license": "MIT",
  "dependencies": {
    "mobx": "^5.5.0",
    "mobx-react": "^5.3.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-hot-loader": "1.3.2",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "react-hot-loader": "^4.3.7",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.7.1"
  }
}
